@model IEnumerable<CRUDwithADO.Models.Employee>
@{

    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Employee</h1>
   <table class="table alert-danger  table-bordered" >
       <tr>
           <th>Id</th>
           <th>Name</th>
           <th>Email</th>
           <th>Mobile</th>
           <th>Gender</th>
           <th>Name</th>
       </tr> 
     @foreach(var emp in Model)
            {
   <tr>
       <td>@emp.Id</td>
       <td>@emp.Name</td>
       <td>@emp.Email</td>
       <td>@emp.Mobile</td>
       <td>@emp.Gender</td>
       <td>@emp.DName</td>
   </tr>
            }
   </table>
</div>
